{
  "name": "admin-domic",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier . --write",
    "preview": "vite preview",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@internationalized/date": "^3.7.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.13",
    "@tanstack/react-query": "^5.67.3",
    "@tanstack/react-router": "^1.114.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.479.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-use": "^17.6.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.13",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "1.0.0-rc.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.32.0",
    "@tanstack/eslint-plugin-query": "^5.67.2",
    "@tanstack/eslint-plugin-router": "^1.114.12",
    "@tanstack/react-query-devtools": "^5.67.3",
    "@tanstack/react-router-devtools": "^1.114.17",
    "@tanstack/router-plugin": "^1.114.17",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.22.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "lint-staged": "^15.5.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "simple-git-hooks": "^2.11.1",
    "type-fest": "^4.37.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node src/scripts/verify-commit.js",
    "pre-push": "pnpm typecheck"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
